{
  "name": "discordjs-multipurpose-bot",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": { "node": ">=18.0.0" },
  "scripts": {
    "manager": "node src/manager.js",
    "start": "node src/worker.js",
    "start:single": "node src/worker.js",
    "deploy-commands": "node src/deploy-commands.js",
    "dev": "nodemon --watch src --ext js --exec \"node src/worker.js\""
  },
  "dependencies": {
    "discord.js": "^14.11.0",
    "@discordjs/rest": "^1.5.0",
    "discord-api-types": "^0.37.0",
    "mongoose": "^7.3.0",
    "pino": "^8.0.0",
    "ioredis": "^5.3.2",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.0"
  }
}
